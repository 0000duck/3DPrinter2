            <controls:AccordionItem  Header="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.GCode}"  HorizontalContentAlignment="Stretch" >
                <Grid HorizontalAlignment="Stretch" Margin="15,7">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>


                    <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.Position}" Margin="0 0 20 0" Grid.Row="0" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
                    
                    <ComboBox Width="300" HorizontalAlignment="Right" Grid.Row="0" Grid.Column="1"  Margin="0,5,0,5" SelectionChanged="Selector_OnSelectionChanged"  >
                        <ComboBoxItem Content="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.StardGCode}" />
                        <ComboBoxItem Content="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.EndGCode}" />
                        <ComboBoxItem Content="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.BeforeExtruderSwitch}" />
                        <ComboBoxItem Content="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.AfterExtruderSwitch}" />
                    </ComboBox>

                    <TextBox x:Name="codeBox" Grid.Row="1" Grid.ColumnSpan="2" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="0,5,0,5" TextWrapping="Wrap" AcceptsReturn="True"/>

                </Grid>
            </controls:AccordionItem>



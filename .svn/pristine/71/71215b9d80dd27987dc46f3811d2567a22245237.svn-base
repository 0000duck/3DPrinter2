                            <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.ShellThinckness}" Margin="0 0 20 0" Grid.Row="0" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
                            <TextBox Grid.Row="0" Grid.Column="1"  HorizontalAlignment="Right"  Width="250" Margin="0,5,0,5" Text="{Binding  Source={x:Static setting:SettingsProvider.Instance}, Mode=TwoWay, Path=SelectedSlicerSettings.extrusionWidth, Converter={StaticResource DoubleToIntConverter}}" />
                            
<!--                            
                            <TextBlock Text="Верх/Низ толщина (мм)" Margin="0 0 20 0" Grid.Row="1" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
                            <TextBox Grid.Row="1" Grid.Column="1"  HorizontalAlignment="Right"  Width="250" Margin="0,5,0,5" Text="{Binding  Source={x:Static setting:SettingsProvider.Instance}, Mode=TwoWay, Path=SelectedSlicerSettings.zAxisFeedrate}" />
-->

                            <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.InfillOverlap}" Margin="0 0 20 0" Grid.Row="2" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />

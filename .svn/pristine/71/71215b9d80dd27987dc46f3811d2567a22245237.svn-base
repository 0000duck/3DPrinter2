                            <ComboBox Width="300" HorizontalAlignment="Right" Grid.Row="3" Grid.Column="1"  Margin="0,5,0,5" SelectedIndex="{Binding  Source={x:Static setting:SettingsProvider.Instance}, Mode=TwoWay, Path=SelectedSlicerSettings.infillPattern}" >
                                <ComboBoxItem Content="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.Automatic}" />
                                <ComboBoxItem Content="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.Grid}" />
                                <ComboBoxItem Content="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.Lines}" />
                                <ComboBoxItem Content="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.Concentric}" />
                            </ComboBox>
<!--
                            <CheckBox Grid.Column="0" Grid.Row="4" Grid.ColumnSpan="2" Content="Solid Top Infill"  Margin="0,5,0,5" IsChecked="{Binding  Source={x:Static setting:SettingsProvider.Instance}, Mode=TwoWay, Path=SelectedPrinterSettings.checkTemp}" />
                            <CheckBox Grid.Column="0" Grid.Row="5" Grid.ColumnSpan="2" Content="Solid Bottom Infill"  Margin="0,5,0,5"  IsChecked="{Binding  Source={x:Static setting:SettingsProvider.Instance}, Mode=TwoWay, Path=SelectedPrinterSettings.logM105}" />
-->
                            <Border BorderThickness="0.5" Height="1" HorizontalAlignment="Stretch" Margin="0,7,0,7" BorderBrush="White" Grid.Row="6" Grid.ColumnSpan="2"></Border>


                            <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.SupportPattern}" Margin="0 0 20 0" Grid.Row="7" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />

                            <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.OverhangAngle}" Margin="0 0 20 0" Grid.Row="8" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
                            
<!--                            
                            <TextBlock Text="Fill Amount (%)" Margin="0 0 20 0" Grid.Row="9" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
                            <TextBox Grid.Row="9" Grid.Column="1"  HorizontalAlignment="Right"  Width="250" Margin="0,5,0,5" Text="{Binding  Source={x:Static setting:SettingsProvider.Instance}, Mode=TwoWay, Path=SelectedSlicerSettings.zAxisFeedrate}" />
-->

                            <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.DistanceXY}" Margin="0 0 20 0" Grid.Row="10" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
                            <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.DistanceZ}" Margin="0 0 20 0" Grid.Row="11" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />

                            <ComboBox Width="300" HorizontalAlignment="Right" Grid.Row="7" Grid.Column="1"  Margin="0,5,0,5" SelectedIndex="{Binding  Source={x:Static setting:SettingsProvider.Instance}, Mode=TwoWay, Path=SelectedSlicerSettings.supportType}" >
                                <ComboBoxItem Content="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.Grid}" />
                                <ComboBoxItem Content="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.Lines}" />
                            </ComboBox>

                            <TextBox Grid.Row="8" Grid.Column="1"  HorizontalAlignment="Right"  Width="250" Margin="0,5,0,5" Text="{Binding  Source={x:Static setting:SettingsProvider.Instance}, Mode=TwoWay, Path=SelectedSlicerSettings.supportAngle}" />
                            <TextBox Grid.Row="10" Grid.Column="1" HorizontalAlignment="Right"  Width="250" Margin="0,5,0,5" Text="{Binding  Source={x:Static setting:SettingsProvider.Instance}, Mode=TwoWay, Path=SelectedSlicerSettings.supportXYDistance, Converter={StaticResource DoubleToIntConverter}}" />
                            <TextBox Grid.Row="11" Grid.Column="1" HorizontalAlignment="Right"  Width="250" Margin="0,5,0,5" Text="{Binding  Source={x:Static setting:SettingsProvider.Instance}, Mode=TwoWay, Path=SelectedSlicerSettings.supportZDistance, Converter={StaticResource DoubleToIntConverter}}" />

                            
                            
                            <Border BorderThickness="0.5" Height="1" HorizontalAlignment="Stretch" Margin="0,7,0,7" BorderBrush="White" Grid.Row="12" Grid.ColumnSpan="2"></Border>

                            <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.SkirtLineCount}" Margin="0 0 20 0" Grid.Row="13" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
                            <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.SkirtDistance}" Margin="0 0 20 0" Grid.Row="14" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
<!--                            
                            <TextBlock Text="Мин. длина края (мм)" Margin="0 0 20 0" Grid.Row="15" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
                            <TextBlock Text="Ширина поля (мм)" Margin="0 0 20 0" Grid.Row="16" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
-->
                            <TextBox Grid.Row="13" Grid.Column="1"  HorizontalAlignment="Right"  Width="250" Margin="0,5,0,5" Text="{Binding  Source={x:Static setting:SettingsProvider.Instance}, Mode=TwoWay, Path=SelectedSlicerSettings.skirtLineCount}" />
                            <TextBox Grid.Row="14" Grid.Column="1"  HorizontalAlignment="Right"  Width="250" Margin="0,5,0,5" Text="{Binding  Source={x:Static setting:SettingsProvider.Instance}, Mode=TwoWay, Path=SelectedSlicerSettings.skirtDistance}" />

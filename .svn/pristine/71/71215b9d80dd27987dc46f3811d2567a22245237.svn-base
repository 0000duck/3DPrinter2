                            <TextBox Grid.Row="16" Grid.Column="1" HorizontalAlignment="Right"  Width="250" Margin="0,5,0,5" Text="{Binding  Source={x:Static setting:SettingsProvider.Instance}, Mode=TwoWay, Path=SelectedSlicerSettings.defaultExtruderTemp}" />
-->
                            

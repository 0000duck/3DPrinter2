<UserControl x:Class="_3DPrinter.setting.view.SlicerSettingsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Layout.Toolkit"
             xmlns:setting="clr-namespace:_3DPrinter.setting"
             xmlns:view="clr-namespace:_3DPrinter.setting.view"
             mc:Ignorable="d" d:DesignHeight="600" d:DesignWidth="800">
    <UserControl.Resources>

        <view:DoubleToIntConverter x:Key="DoubleToIntConverter"/>

    </UserControl.Resources>
    
    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch"  Margin="20,0,20,20">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.SlicerProfile}" Margin="0,21,0,20" Grid.Row="1" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />

        <StackPanel Orientation="Horizontal"  Grid.Row="1" Grid.Column="1" HorizontalAlignment="Right">
            <ComboBox x:Name="printerComboBox" Width="250" HorizontalAlignment="Right" Margin="0,15,0,15" KeyUp="PrinterComboBox_OnKeyUp" IsEditable="True" SelectedIndex="{Binding  Source={x:Static setting:SettingsProvider.Instance}, Mode=TwoWay, Path=SelectedSlicerSettingsIndex}" ItemsSource="{Binding  Source={x:Static setting:SettingsProvider.Instance}, Mode=OneWay, Path=SlicerNameList}" />
            <Button Margin="0,15,0,15" HorizontalAlignment="Right" Click="ButtonBase_OnClick" ToolTip="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.Delete}">
                <Image HorizontalAlignment="Center" VerticalAlignment="Center" Source="../../images/del_printer.png" Height="20" Width="20" Stretch="Fill" />
            </Button>
        </StackPanel>

        <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.SlicerSettings}" Margin="0 0 0 20" HorizontalAlignment="Left" Style="{StaticResource HeaderTextBlockStyle}" />

        <controls:Accordion HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Grid.Row="2" Padding="0" Grid.ColumnSpan="2">

            <controls:AccordionItem Header="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.Speed}" >
                <Grid HorizontalAlignment="Stretch" Margin="15,7">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>

                    <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.PrintSpeed}" Margin="0 0 20 0" Grid.Row="1" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
                    <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.TravelSpeed}" Margin="0 0 20 0" Grid.Row="2" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />

                    <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.FirstLayerSpeed}" Margin="0 0 20 0" Grid.Row="3" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
                    <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.OuterPerimeterSpeed}" Margin="0 0 20 0" Grid.Row="4" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />

                    <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.InnerPerimeterSpeed}" Margin="0 0 20 0" Grid.Row="5" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
                    <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.InfillSpeed}" Margin="0 0 20 0" Grid.Row="6" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
                    <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.SkinInfillSpeed}" Margin="0 0 20 0" Grid.Row="7" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
<!--
                    <TextBlock Text="Медленно" Margin="10 5 10 2" Grid.Row="0" Grid.Column="1" HorizontalAlignment="Center"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
                    <TextBlock Text="Бысро" Margin="10 5 10 2" Grid.Row="0" Grid.Column="2"  HorizontalAlignment="Center"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
-->

                    <TextBox Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2"  HorizontalAlignment="Stretch"   Margin="10,5,10,5" Text="{Binding Source={x:Static setting:SettingsProvider.Instance}, Path=SelectedSlicerSettings.printSpeed, Mode=TwoWay}" />

                    <TextBox Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="2"  HorizontalAlignment="Stretch"  Margin="10,5,10,5" Text="{Binding Source={x:Static setting:SettingsProvider.Instance}, Path=SelectedSlicerSettings.moveSpeed, Mode=TwoWay}"/>

                    <TextBox Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="2"  HorizontalAlignment="Stretch"  Margin="10,5,10,5" Text="{Binding Source={x:Static setting:SettingsProvider.Instance}, Path=SelectedSlicerSettings.initialLayerSpeed, Mode=TwoWay}"/>

                    <TextBox Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="2"  HorizontalAlignment="Stretch"  Margin="10,5,10,5" Text="{Binding Source={x:Static setting:SettingsProvider.Instance}, Path=SelectedSlicerSettings.inset0Speed, Mode=TwoWay}"/>

                    <TextBox Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="2"  HorizontalAlignment="Stretch"  Margin="10,5,10,5" Text="{Binding Source={x:Static setting:SettingsProvider.Instance}, Path=SelectedSlicerSettings.insetXSpeed, Mode=TwoWay}"/>

                    <TextBox Grid.Row="6" Grid.Column="1" Grid.ColumnSpan="2"  HorizontalAlignment="Stretch"  Margin="10,5,10,5" Text="{Binding Source={x:Static setting:SettingsProvider.Instance}, Path=SelectedSlicerSettings.infillSpeed, Mode=TwoWay}"/>

                    <TextBox Grid.Row="7" Grid.Column="1" Grid.ColumnSpan="2"  HorizontalAlignment="Stretch"  Margin="10,5,10,5" Text="{Binding Source={x:Static setting:SettingsProvider.Instance}, Path=SelectedSlicerSettings.skinSpeed, Mode=TwoWay}"/>
                </Grid>
            </controls:AccordionItem>

            <controls:AccordionItem Header="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.Quality}" >
                <Grid HorizontalAlignment="Stretch" Margin="15,7">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>

                    <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.LayerHeight}" Margin="0 0 20 0" Grid.Row="0" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
                    <TextBlock Text="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.FirstLayerHeight}" Margin="0 0 20 0" Grid.Row="1" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
<!--
                    <TextBlock Text="Толщина первого слоя (%)" Margin="0 0 20 0" Grid.Row="2" HorizontalAlignment="Left"  Style="{StaticResource HeaderTextBlockStyle}" FontSize="14" VerticalAlignment="Center" />
-->

                    <TextBox Grid.Row="0" Grid.Column="1"  HorizontalAlignment="Right"  Width="250" Margin="0,5,0,5" Text="{Binding Source={x:Static setting:SettingsProvider.Instance}, Path=SelectedSlicerSettings.layerThickness, Mode=TwoWay, Converter={StaticResource DoubleToIntConverter}}" />
                    <TextBox Grid.Row="1" Grid.Column="1"  HorizontalAlignment="Right"  Width="250" Margin="0,5,0,5" Text="{Binding Source={x:Static setting:SettingsProvider.Instance}, Path=SelectedSlicerSettings.initialLayerThickness, Mode=TwoWay, Converter={StaticResource DoubleToIntConverter}}" />
<!--                    
                    <TextBox Grid.Row="2" Grid.Column="1"  HorizontalAlignment="Right"  Width="250" Margin="0,5,0,5" />
-->

                </Grid>
            </controls:AccordionItem>

            <controls:AccordionItem Header="{Binding Source={x:Static setting:Localization.Instance}, Path=CurrentLanguage.Structures}" Margin="0" >
                <ScrollViewer VerticalScrollBarVisibility="Auto"  HorizontalScrollBarVisibility="Hidden" Margin="0" >
                    <StackPanel Margin="0">
                        <Grid HorizontalAlignment="Stretch" Margin="15,7">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
